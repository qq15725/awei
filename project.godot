; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="aweilands"
run/main_scene="res://text-scenes/game-world.tscn"
boot_splash/image="res://assets/none.png"
config/icon="res://icon.png"

[autoload]

Global="*res://dist/global.jsx"

[display]

window/size/width=1280
window/size/height=720
window/dpi/allow_hidpi=true
window/stretch/aspect="keep"

[gui]

theme/use_hidpi=true
theme/custom_font="res://font.tres"

[physics]

2d/default_gravity=0

[rendering]

quality/filters/use_nearest_mipmap_filter=true
environment/default_environment="res://env.tres"
